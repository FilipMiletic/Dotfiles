# Source Prezto.
if [[ -s "${ZDOTDIR:-$HOME}/.zprezto/init.zsh" ]]; then
  source "${ZDOTDIR:-$HOME}/.zprezto/init.zsh"
fi

# Customize to your needs...
export TERM=screen-256color
# alias tmux="TERM=xterm-256color tmux"
alias tmux="TERM=screen-256color tmux"

